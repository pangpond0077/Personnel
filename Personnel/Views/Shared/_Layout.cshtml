    <script src="~/plugins/select2/dist/js/select2.full.min.js"></script>
    <script src="~/plugins/select2/dist/js/select2.full.js"></script>

    <script src="~/js/plugins/typehead/bootstrap3-typeahead.min.js"></script>

    <!-- Idle Timer plugin -->
    <script src="~/js/plugins/idle-timer/idle-timer.min.js"></script>
    <link href="~/css/bootstrap-glyphicons.min.css" rel="stylesheet" type="text/css" />

    <script src="~/plugins/Chart.js/Chart.bundle.js"></script>
    <script src="~/plugins/Chart.js/Chart.js"></script>

    <!--Fullcalendar-->
    <script src="~/plugins/fullcalendar/dist/fullcalendar.min.js"></script>

    <!-- Steps -->
    <script src="~/js/plugins/steps/jquery.steps.min.js"></script>

    <!-- Jquery Validate -->
    <script src="~/js/plugins/validate/jquery.validate.min.js"></script>





    <script src="~/js/site.js" asp-append-version="true"></script>


    <script>
        $(function () {
            pdfMake.fonts = {
                THSarabun: {
                    normal: 'THSarabun.ttf',
                    bold: 'THSarabun Bold.ttf',
                    italics: 'THSarabun Italic.ttf',
                    bolditalics: 'THSarabun BoldItalic.ttf'
                },
                Roboto: {
                    normal: 'Roboto-Regular.ttf',
                    bold: 'Roboto-Medium.ttf',
                    italics: 'Roboto-Italic.ttf',
                    bolditalics: 'Roboto-MediumItalic.ttf'
                }
            }
        });

    </script>

    @RenderSection("Scripts", required: false)
</body>
</html>
